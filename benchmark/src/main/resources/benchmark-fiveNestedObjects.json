{
  "name": "benchmark-fiveNestedObjects",
  "description": "No description",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "/tmp/checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in-kafka",
    "type": "Kafka",
    "configuration": {
      "topics": [{"topic": "benchmark-fiveNestedObjects", "partition" : "1"}],
      "zookeeper.connect": [{"host": "zk.demo.stratio.com", "port": "2181"}],
      "kafkaParams.group.id": "KafkaCluster"
    }
  }
,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"orderId"},
        {"name":"timestamp"},
        {"name":"clientId"},
        {"name":"city"},
        {"name":"country"},
        {"name":"continent"},
        {"name":"planet"},
        {"name":"galaxy"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "orderId": "/order_id",
                  "timestamp": "/timestamp",
                  "clientId": "/client_id",
                  "city": "/city/name",
                  "country": "/city/country/name",
                  "continent": "/city/country/continent/name",
                  "planet": "/city/country/continent/planet/name",
                  "galaxy": "/city/country/continent/planet/galaxy/name"
                }
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "outputFields": [
        {"name":"timestamp"}
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "writer": {
        "outputs" : ["out-print"]
      },
      "dimensions": [
        {
          "name": "city",
          "field": "city"
        },
        {
          "name": "country",
          "field": "country"
        },
        {
          "name": "continent",
          "field": "continent"
        },
        {
          "name": "planet",
          "field": "planet"
        },
        {
          "name": "galaxy",
          "field": "galaxy"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {

      }
    }
  ]
}
